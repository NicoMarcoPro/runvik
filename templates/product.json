radius:9999px;\n  border:1px solid var(--sg-orange); background:transparent; color:var(--sg-orange);\n  cursor:pointer; transition:transform .08s ease, background .2s ease, color .2s ease;\n}\n.sizeguide__btn:hover{ background:var(--sg-orange); color:#fff; transform:translateY(-1px); }\n\n/* Overlay centr√© */\n.sizeguide__overlay{\n  position:fixed; inset:0; display:flex; align-items:center; justify-content:center;\n  padding:16px; background:rgba(0,0,0,.5); z-index:999999;\n  opacity:0; transition:opacity .18s ease;\n}\n.sizeguide__overlay.is-open{ opacity:1; }\n\n/* Carte modal */\n.sizeguide__modal{\n  width:min(92vw, 720px); max-height:85vh; overflow:auto;\n  background:var(--sg-bg); color:var(--sg-text);\n  border-radius:16px; box-shadow:0 10px 30px rgba(0,0,0,.2);\n  padding:20px 18px 22px; transform:translateY(8px); transition:transform .18s ease, opacity .18s ease; opacity:.98;\n}\n.sizeguide__overlay.is-open .sizeguide__modal{ transform:translateY(0); opacity:1; }\n\n.sizeguide__title{ margin:2px 32px 6px 2px; font-size:18px; font-weight:700; }\n.sizeguide__note{ margin:0 0 12px 0; font-size:13px; color:var(--sg-muted); }\n\n/* Close */\n.sizeguide__close{\n  position:sticky; top:0; float:right; margin:-4px -4px 8px 8px;\n  border:none; background:transparent; font-size:24px; line-height:1; cursor:pointer; color:#999;\n}\n.sizeguide__close:hover{ color:#333; }\n\n/* Table */\n.sizeguide__tablewrap{ width:100%; overflow-x:hidden; }\n.sizeguide__table{\n  width:100%; border-collapse:collapse; table-layout:fixed; font-size:14px; border-radius:12px; overflow:hidden;\n}\n.sizeguide__table th, .sizeguide__table td{\n  padding:10px 8px; text-align:center; word-wrap:break-word; border-bottom:1px solid #eee;\n}\n.sizeguide__table thead th{\n  position:sticky; top:0; background:var(--sg-orange); color:#fff; font-weight:700;\n}\n.sizeguide__table tbody tr:nth-child(even){ background:#fafafa; }\n\n.sizeguide__tips{ margin:12px 0 4px; font-size:12px; color:var(--sg-muted); }\n</style>\n\n<script>\n(function(){\n  document.querySelectorAll('.sizeguide[data-sg]').forEach((root)=>{\n    const btn     = root.querySelector('.sizeguide__btn');\n    const overlay = root.querySelector('.sizeguide__overlay');\n    const modal   = root.querySelector('.sizeguide__modal');\n    const closeBtn= root.querySelector('.sizeguide__close');\n\n    if(!btn || !overlay) return;\n\n    const open = () => {\n      overlay.hidden = false;\n      requestAnimationFrame(()=> overlay.classList.add('is-open'));\n      document.documentElement.style.overflow = 'hidden';\n      btn.setAttribute('aria-expanded','true');\n      modal && modal.focus();\n    };\n\n    const close = () => {\n      overlay.classList.remove('is-open');\n      const onEnd = () => {\n        overlay.hidden = true;\n        overlay.removeEventListener('transitionend', onEnd);\n      };\n      overlay.addEventListener('transitionend', onEnd);\n      document.documentElement.style.overflow = '';\n      btn.setAttribute('aria-expanded','false');\n    };\n\n    btn.addEventListener('click', open);\n    closeBtn && closeBtn.addEventListener('click', close);\n    overlay.addEventListener('click', (e)=> { if(e.target === overlay) close(); });\n    document.addEventListener('keydown', (e)=> { if(!overlay.hidden && e.key === 'Escape') close(); }, { passive:true });\n  });\n})();\n</script>"
