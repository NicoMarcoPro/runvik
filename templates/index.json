            "liquid": "<!DOCTYPE html>\n<html lang=\"fr\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Runvik - Section Premium</title>\n<style>\n  /* ==================== VARIABLES & RESET ==================== */\n  * {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n  }\n\n  :root {\n    /* Couleurs Premium */\n    --rv-primary: #000000;\n    --rv-secondary: #ffffff;\n    --rv-accent: #0066FF;\n    --rv-gray-100: #FAFAFA;\n    --rv-gray-200: #F5F5F5;\n    --rv-gray-300: #E8E8E8;\n    --rv-gray-400: #D4D4D4;\n    --rv-gray-500: #737373;\n    --rv-gray-600: #525252;\n    --rv-success: #16A34A;\n    \n    /* Typographie Pro */\n    --rv-font-primary: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, sans-serif;\n    --rv-font-display: \"SF Pro Display\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, sans-serif;\n    \n    /* Espacements */\n    --rv-space-xs: 4px;\n    --rv-space-sm: 8px;\n    --rv-space-md: 16px;\n    --rv-space-lg: 24px;\n    --rv-space-xl: 32px;\n    --rv-space-2xl: 48px;\n    --rv-space-3xl: 64px;\n    \n    /* Animations Premium */\n    --rv-transition-fast: 150ms cubic-bezier(0.4, 0, 0.2, 1);\n    --rv-transition-base: 250ms cubic-bezier(0.4, 0, 0.2, 1);\n    --rv-transition-slow: 350ms cubic-bezier(0.4, 0, 0.2, 1);\n    --rv-ease-out: cubic-bezier(0, 0, 0.2, 1);\n    --rv-ease-in-out: cubic-bezier(0.4, 0, 1, 1);\n    \n    /* Ombres subtiles */\n    --rv-shadow-sm: 0 1px 2px 0 rgba(0, 0, 0, 0.05);\n    --rv-shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n    --rv-shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.1);\n    --rv-shadow-xl: 0 20px 25px -5px rgba(0, 0, 0, 0.1);\n    \n    /* Bordures */\n    --rv-radius-sm: 4px;\n    --rv-radius-md: 8px;\n    --rv-radius-lg: 12px;\n    --rv-radius-xl: 16px;\n    --rv-radius-full: 9999px;\n  }\n\n  /* ==================== CONTENEUR PRINCIPAL ==================== */\n  .rv-trust-section {\n    width: 100%;\n    padding: var(--rv-space-2xl) 0;\n    background: var(--rv-secondary);\n    font-family: var(--rv-font-primary);\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n  }\n\n  .rv-container {\n    max-width: 1200px;\n    margin: 0 auto;\n    padding: 0 var(--rv-space-lg);\n  }\n\n  /* ==================== BANNIÈRE EXPÉDITION PREMIUM ==================== */\n  .rv-shipping-banner {\n    background: linear-gradient(135deg, var(--rv-gray-100) 0%, var(--rv-gray-200) 100%);\n    border-radius: var(--rv-radius-lg);\n    padding: var(--rv-space-md) var(--rv-space-lg);\n    margin-bottom: var(--rv-space-xl);\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    gap: var(--rv-space-md);\n    position: relative;\n    overflow: hidden;\n    transition: all var(--rv-transition-base);\n    cursor: default;\n    border: 1px solid var(--rv-gray-300);\n  }\n\n  .rv-shipping-banner::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: -100%;\n    width: 100%;\n    height: 100%;\n    background: linear-gradient(90deg, transparent, rgba(255,255,255,0.4), transparent);\n    transition: left var(--rv-transition-slow);\n  }\n\n  .rv-shipping-banner:hover::before {\n    left: 100%;\n  }\n\n  .rv-shipping-banner:hover {\n    transform: translateY(-2px);\n    box-shadow: var(--rv-shadow-md);\n    background: linear-gradient(135deg, var(--rv-gray-200) 0%, var(--rv-gray-300) 100%);\n  }\n\n  .rv-shipping-icon {\n    width: 24px;\n    height: 24px;\n    flex-shrink: 0;\n    color: var(--rv-primary);\n    animation: truck-bounce 2s ease-in-out infinite;\n  }\n\n  @keyframes truck-bounce {\n    0%, 100% { transform: translateX(0); }\n    50% { transform: translateX(3px); }\n  }\n\n  .rv-shipping-text {\n    font-size: 15px;\n    font-weight: 600;\n    color: var(--rv-primary);\n    letter-spacing: -0.01em;\n    line-height: 1.2;\n  }\n\n  /* ==================== GRILLE DES AVANTAGES PREMIUM ==================== */\n  .rv-features-grid {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    gap: var(--rv-space-md);\n    margin-top: var(--rv-space-lg);\n  }\n\n  .rv-feature-card {\n    background: var(--rv-gray-100);\n    border-radius: var(--rv-radius-xl);\n    padding: var(--rv-space-xl) var(--rv-space-lg);\n    text-align: center;\n    position: relative;\n    overflow: hidden;\n    transition: all var(--rv-transition-base);\n    cursor: pointer;\n    border: 1px solid transparent;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    min-height: 160px;\n  }\n\n  .rv-feature-card::before {\n    content: '';\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    width: 0;\n    height: 0;\n    border-radius: 50%;\n    background: rgba(0, 102, 255, 0.05);\n    transform: translate(-50%, -50%);\n    transition: width var(--rv-transition-slow), height var(--rv-transition-slow);\n  }\n\n  .rv-feature-card:hover::before {\n    width: 300px;\n    height: 300px;\n  }\n\n  .rv-feature-card:hover {\n    transform: translateY(-4px) scale(1.02);\n    box-shadow: var(--rv-shadow-lg);\n    border-color: var(--rv-gray-300);\n    background: var(--rv-secondary);\n  }\n\n  .rv-feature-icon-wrapper {\n    width: 48px;\n    height: 48px;\n    margin: 0 auto var(--rv-space-md);\n    background: var(--rv-secondary);\n    border-radius: var(--rv-radius-full);\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    position: relative;\n    z-index: 1;\n    transition: all var(--rv-transition-base);\n  }\n\n  .rv-feature-card:hover .rv-feature-icon-wrapper {\n    transform: scale(1.1) rotate(5deg);\n    background: var(--rv-accent);\n  }\n\n  .rv-feature-icon {\n    width: 24px;\n    height: 24px;\n    color: var(--rv-primary);\n    transition: all var(--rv-transition-base);\n  }\n\n  .rv-feature-card:hover .rv-feature-icon {\n    color: var(--rv-secondary);\n    transform: scale(1.1);\n  }\n\n  .rv-feature-title {\n    font-size: 15px;\n    font-weight: 600;\n    color: var(--rv-primary);\n    line-height: 1.4;\n    letter-spacing: -0.02em;\n    margin: 0;\n    position: relative;\n    z-index: 1;\n  }\n\n  .rv-feature-subtitle {\n    font-size: 13px;\n    color: var(--rv-gray-500);\n    margin-top: var(--rv-space-xs);\n    font-weight: 400;\n    opacity: 0;\n    transform: translateY(-10px);\n    transition: all var(--rv-transition-base);\n  }\n\n  .rv-feature-card:hover .rv-feature-subtitle {\n    opacity: 1;\n    transform: translateY(0);\n  }\n\n  /* ==================== ANIMATIONS D'ENTRÉE ==================== */\n  @keyframes fade-in-up {\n    from {\n      opacity: 0;\n      transform: translateY(20px);\n    }\n    to {\n      opacity: 1;\n      transform: translateY(0);\n    }\n  }\n\n  .rv-shipping-banner {\n    animation: fade-in-up 0.6s var(--rv-ease-out);\n  }\n\n  .rv-feature-card {\n    animation: fade-in-up 0.6s var(--rv-ease-out) backwards;\n  }\n\n  .rv-feature-card:nth-child(1) { animation-delay: 0.1s; }\n  .rv-feature-card:nth-child(2) { animation-delay: 0.2s; }\n  .rv-feature-card:nth-child(3) { animation-delay: 0.3s; }\n\n  /* ==================== RESPONSIVE TABLETTE ==================== */\n  @media (max-width: 768px) {\n    .rv-container {\n      padding: 0 var(--rv-space-md);\n    }\n\n    .rv-trust-section {\n      padding: var(--rv-space-xl) 0;\n    }\n\n    .rv-features-grid {\n      gap: var(--rv-space-sm);\n    }\n\n    .rv-feature-card {\n      padding: var(--rv-space-lg) var(--rv-space-md);\n      min-height: 140px;\n    }\n\n    .rv-feature-title {\n      font-size: 14px;\n    }\n  }\n\n  /* ==================== RESPONSIVE MOBILE ==================== */\n  @media (max-width: 480px) {\n    .rv-shipping-banner {\n      padding: var(--rv-space-md) var(--rv-space-md);\n      border-radius: var(--rv-radius-md);\n    }\n\n    .rv-shipping-text {\n      font-size: 14px;\n    }\n\n    .rv-features-grid {\n      grid-template-columns: 1fr;\n      gap: var(--rv-space-sm);\n    }\n\n    .rv-feature-card {\n      padding: var(--rv-space-lg);\n      min-height: auto;\n      flex-direction: row;\n      text-align: left;\n      justify-content: flex-start;\n    }\n\n    .rv-feature-icon-wrapper {\n      margin: 0 var(--rv-space-md) 0 0;\n      flex-shrink: 0;\n    }\n\n    .rv-feature-content {\n      flex: 1;\n    }\n\n    .rv-feature-title {\n      font-size: 15px;\n    }\n\n    .rv-feature-subtitle {\n      opacity: 1;\n      transform: none;\n      margin-top: var(--rv-space-xs);\n    }\n\n    .rv-feature-card:hover {\n      transform: translateY(-2px) scale(1);\n    }\n  }\n\n  /* ==================== MODE SOMBRE (BONUS) ==================== */\n  @media (prefers-color-scheme: dark) {\n    :root {\n      --rv-primary: #ffffff;\n      --rv-secondary: #000000;\n      --rv-gray-100: #171717;\n      --rv-gray-200: #262626;\n      --rv-gray-300: #404040;\n      --rv-gray-500: #a3a3a3;\n    }\n\n    .rv-shipping-banner {\n      background: linear-gradient(135deg, #1a1a1a 0%, #262626 100%);\n      border-color: var(--rv-gray-300);\n    }\n\n    .rv-feature-card {\n      background: #0a0a0a;\n    }\n\n    .rv-feature-card:hover {\n      background: #171717;\n    }\n  }\n\n  /* ==================== OPTIMISATIONS PERFORMANCES ==================== */\n  @media (prefers-reduced-motion: reduce) {\n    * {\n      animation-duration: 0.01ms !important;\n      animation-iteration-count: 1 !important;\n      transition-duration: 0.01ms !important;\n    }\n  }\n</style>\n</head>\n<body>\n\n<!-- SECTION PRINCIPALE -->\n<div class=\"rv-trust-section\">\n  <div class=\"rv-container\">\n    \n    <!-- BANNIÈRE EXPÉDITION DYNAMIQUE -->\n    <div id=\"rv-shipping-banner\" class=\"rv-shipping-banner\">\n      <svg class=\"rv-shipping-icon\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n        <rect x=\"1\" y=\"3\" width=\"15\" height=\"13\" rx=\"2\"/>\n        <path d=\"M16 8h4l3 4v4h-7V8z\"/>\n        <circle cx=\"5.5\" cy=\"17.5\" r=\"2.5\"/>\n        <circle cx=\"18.5\" cy=\"17.5\" r=\"2.5\"/>\n      </svg>\n      <span class=\"rv-shipping-text\" id=\"rv-shipping-text\">Livraison rapide et sécurisée</span>\n    </div>\n\n    <!-- GRILLE DES AVANTAGES -->\n    <div class=\"rv-features-grid\">\n      \n      <!-- Garantie -->\n      <div class=\"rv-feature-card\">\n        <div class=\"rv-feature-icon-wrapper\">\n          <svg class=\"rv-feature-icon\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n            <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"/>\n            <path d=\"M9 12l2 2 4-4\"/>\n          </svg>\n        </div>\n        <div class=\"rv-feature-content\">\n          <h3 class=\"rv-feature-title\">Satisfait ou remboursé</h3>\n          <p class=\"rv-feature-subtitle\">30 jours pour changer d'avis</p>\n        </div>\n      </div>\n\n      <!-- Qualité -->\n      <div class=\"rv-feature-card\">\n        <div class=\"rv-feature-icon-wrapper\">\n          <svg class=\"rv-feature-icon\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n            <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n            <path d=\"M9 12l2 2 4-4\"/>\n          </svg>\n        </div>\n        <div class=\"rv-feature-content\">\n          <h3 class=\"rv-feature-title\">Testé et approuvé<br>par les coureurs</h3>\n          <p class=\"rv-feature-subtitle\">Performance validée sur le terrain</p>\n        </div>\n      </div>\n\n      <!-- Paiement -->\n      <div class=\"rv-feature-card\">\n        <div class=\"rv-feature-icon-wrapper\">\n          <svg class=\"rv-feature-icon\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n            <rect x=\"2\" y=\"5\" width=\"20\" height=\"14\" rx=\"2\"/>\n            <line x1=\"2\" y1=\"10\" x2=\"22\" y2=\"10\"/>\n          </svg>\n        </div>\n        <div class=\"rv-feature-content\">\n          <h3 class=\"rv-feature-title\">Paiement<br>100% sécurisé</h3>\n          <p class=\"rv-feature-subtitle\">Cryptage SSL de vos données</p>\n        </div>\n      </div>\n\n    </div>\n  </div>\n</div>\n\n<!-- SCRIPT GÉOLOCALISATION OPTIMISÉ -->\n<script>\n(function() {\n  'use strict';\n  \n  // Configuration des messages par zone\n  const shippingMessages = {\n    FR: '🚀 Expédition rapide depuis Lyon – 24-48h',\n    EU: '🇪🇺 Livraison Europe – 2-4 jours ouvrés',\n    GB: '🇬🇧 Livraison Royaume-Uni – 3-5 jours',\n    INTL: '🌍 Livraison internationale – 5-7 jours'\n  };\n  \n  const EU_COUNTRIES = ['BE','LU','DE','ES','IT','NL','PT','AT','IE','FI','SE','DK','PL','CZ','HU','RO','BG','HR','SI','SK','LT','LV','EE','CY','MT','GR'];\n  \n  // Fonction de mise à jour du message\n  function updateShippingMessage(countryCode) {\n    const textElement = document.getElementById('rv-shipping-text');\n    if (!textElement) return;\n    \n    let message = shippingMessages.INTL;\n    \n    if (countryCode === 'FR') {\n      message = shippingMessages.FR;\n    } else if (EU_COUNTRIES.includes(countryCode)) {\n      message = shippingMessages.EU;\n    } else if (countryCode === 'GB') {\n      message = shippingMessages.GB;\n    }\n    \n    // Animation de transition\n    textElement.style.opacity = '0';\n    setTimeout(() => {\n      textElement.textContent = message;\n      textElement.style.opacity = '1';\n    }, 150);\n  }\n  \n  // Détection avec fallback intelligent\n  async function detectLocation() {\n    try {\n      // Tentative avec ipapi.co\n      const response = await fetch('https://ipapi.co/json/', {\n        signal: AbortSignal.timeout(3000) // Timeout de 3 secondes\n      });\n      \n      if (response.ok) {\n        const data = await response.json();\n        updateShippingMessage(data.country_code || 'INTL');\n      } else {\n        throw new Error('API indisponible');\n      }\n    } catch (error) {\n      // Fallback basé sur le fuseau horaire\n      const timezone = Intl.DateTimeFormat().resolvedOptions().timeZone;\n      \n      if (timezone.includes('Paris') || timezone.includes('Brussels') || timezone.includes('Amsterdam')) {\n        updateShippingMessage('EU');\n      } else if (timezone.includes('London')) {\n        updateShippingMessage('GB');\n      } else {\n        updateShippingMessage('INTL');\n      }\n    }\n  }\n  \n  // Initialisation au chargement\n  if (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', detectLocation);\n  } else {\n    detectLocation();\n  }\n})();\n</script>\n\n</body>\n</html>"
