\n  flex-direction: column;\n  gap: 4px;\n}\n\n.discount {\n  font-size: 20px;\n  font-weight: 900;\n  color: #333;\n  line-height: 1;\n}\n\n.label {\n  font-size: 9px;\n  color: #666;\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 0.2px;\n}\n\n/* Mobile optimis√© */\n@media (max-width: 480px) {\n  .quantity-promotion-compact {\n    margin: 10px 0;\n    padding: 0 5px;\n  }\n  \n  .promo-wrapper {\n    gap: 6px;\n  }\n  \n  .promo-card {\n    padding: 10px 6px;\n    min-height: 75px;\n  }\n  \n  .promo-badge {\n    font-size: 9px;\n    padding: 3px 5px;\n    margin-bottom: 6px;\n  }\n  \n  .discount {\n    font-size: 18px;\n  }\n  \n  .label {\n    font-size: 8px;\n  }\n}\n\n/* Tr√®s petits √©crans */\n@media (max-width: 360px) {\n  .promo-card {\n    padding: 8px 4px;\n    min-height: 70px;\n  }\n  \n  .promo-badge {\n    font-size: 8px;\n    padding: 2px 4px;\n  }\n  \n  .discount {\n    font-size: 16px;\n  }\n  \n  .label {\n    font-size: 7px;\n  }\n}\n\n/* Animation de s√©lection */\n@keyframes selectPulse {\n  0% { transform: scale(1); }\n  50% { transform: scale(1.02); }\n  100% { transform: scale(1); }\n}\n\n.promo-card.selected {\n  border-color: #ff4500;\n  background: #fff5f0;\n  animation: selectPulse 0.3s ease;\n}\n</style>\n\n<!-- JavaScript pour interaction dynamique -->\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  const promoCards = document.querySelectorAll('.promo-card');\n  \n  // Fonction pour mettre √† jour la s√©lection\n  function updateSelection(quantity) {\n    promoCards.forEach(card => {\n      card.classList.remove('active', 'selected');\n      const cardQuantity = parseInt(card.dataset.quantity);\n      \n      if (quantity >= 6 && cardQuantity === 6) {\n        card.classList.add('active', 'selected');\n      } else if (quantity >= 4 && quantity < 6 && cardQuantity === 4) {\n        card.classList.add('active', 'selected');\n      } else if (quantity >= 2 && quantity < 4 && cardQuantity === 2) {\n        card.classList.add('active', 'selected');\n      }\n    });\n  }\n  \n  // √âcouter les changements de quantit√©\n  const quantityInput = document.querySelector('input[name=\"quantity\"]');\n  if (quantityInput) {\n    quantityInput.addEventListener('input', function() {\n      updateSelection(parseInt(this.value || 1));\n    });\n    \n    // Initialiser\n    updateSelection(parseInt(quantityInput.value || 1));\n  }\n  \n  // Permettre la s√©lection en cliquant sur les cartes\n  promoCards.forEach(card => {\n    card.addEventListener('click', function() {\n      const targetQuantity = parseInt(this.dataset.quantity);\n      if (quantityInput) {\n        quantityInput.value = targetQuantity;\n        quantityInput.dispatchEvent(new Event('change'));\n        updateSelection(targetQuantity);\n      }\n    });\n  });\n});\n</script>"
